{
  type_urn: "urn:x-mfg:operation:characterize:shear-test",
  name: "Shear Test Operation",
  description: "A test of shear stress response",
  parent_urns: ["urn:x-mfg:operation:default"],
  attachments: {
    ":sample": [
      [
        ":material:sample",
        {
          ":specimens": [
            [
              ":material:part",
              {
                ":test-dimensions": [":digital:document:named-dimensions"],
                ":test-measurements": [":digital:frame:force-displacement"],
                ":images": [":digital:file"],
                ":notes": [":digital:file", ":digital:text"],
                ":tested-specimen": [":material:part"],
                ":computed-properties": [
                  [
                    ":digital:document:shear-properties",
                    {
                      ":notes": [":digital:file", ":digital:text"],
                    },
                  ],
                ],
              },
            ],
          ],
          ":computed-properties": [
            [
              ":digital:document:shear-properties",
              {
                ":notes": [":digital:file", ":digital:text"],
              },
            ],
          ],
        },
      ],
    ],
  },
  provenance: {
    steps: [
      {
        name: ":aggregate",
        context: ":sample.*",
        from_artifacts: [".:specimens.*"],
        to_artifacts: ["."],
      },
      {
        name: ":dimension",
        context: ":sample.*.:specimens.*",
        from_artifacts: ["."],
        to_artifacts: [".:test-dimensions.*"],
      },
      {
        name: ":test",
        context: ":sample.*.:specimens.*",
        from_artifacts: ["."],
        to_artifacts: [
          ".:test-measurements.*",
          ".:tested-specimen.*",
          ".:images.*",
          ".:notes.*",
        ],
      },
      {
        name: ":compute",
        context: ":sample.*.:specimens.*",
        from_artifacts: ["."],
        to_artifacts: [
          ".:computed-properties.*",
          ".:computed-properties.*.:notes.*",
        ],
      },
      {
        name: ":compute",
        context: ":sample.*",
        from_artifacts: ["."],
        to_artifacts: [
          ".:computed-properties.*",
          ".:computed-properties.*.:notes.*",
        ],
      },
    ],
  },
  services: {
    class_qualname: "mppw.services.operations.characterize_shear_test_services:CharacterizeShearTestServices",
  },
}
